<div id="gradient">
<div class="container">
    <div class="row">
      <div class="col-6">        
        <form class="chat-room-form">
          <div class="form-group">
            <label for="roomName">Room</label>
            <input type="text" placeholder="Enter room name" id="roomName" class="form-control" name="roomName">
            <div id="nameError" class="invalid-feedback"></div>
          </div>

          <button id="create-btn" type="submit" class="submit btn btn-primary">Create Room</button>
          {{!-- <button id="join-btn" type="submit" class="submit btn btn-primary">Join Room</button> --}}
        </form>
      </div>
    </div>
</div>
<div class="roomList">
    <ul class="rooms">
        {{#each Rooms}}
            <li><a href="/video?method=join&roomName={{room_name}}">{{room_name}}</a></li>
        {{/each}}
    </ul>
</div>
</div>
<script src="scripts/simplewebrtc/out/simplewebrtc-with-adapter.bundle.js"></script>
<script src="/socket.io/socket.io.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.12/handlebars.min.js"></script>
<script src="js/createrooms.js"></script>
<link rel='stylesheet' href='styles/style.css' /> 

